FRAMEWORK_SEARCH_PATHS = $(inherited) "$PODS_FRAMEWORK_BUILD_PATH"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$PODS_FRAMEWORK_BUILD_PATH/AlamoImage.framework/Headers" -iquote "$PODS_FRAMEWORK_BUILD_PATH/Alamofire.framework/Headers" -iquote "$PODS_FRAMEWORK_BUILD_PATH/JGMethodSwizzler.framework/Headers" -iquote "$PODS_FRAMEWORK_BUILD_PATH/MZAppearance.framework/Headers" -iquote "$PODS_FRAMEWORK_BUILD_PATH/MZFormSheetPresentationController.framework/Headers" -iquote "$PODS_FRAMEWORK_BUILD_PATH/SwiftyJSON.framework/Headers"
OTHER_LDFLAGS = $(inherited) -ObjC -framework "AlamoImage" -framework "Alamofire" -framework "JGMethodSwizzler" -framework "MZAppearance" -framework "MZFormSheetPresentationController" -framework "SwiftyJSON"
OTHER_LIBTOOLFLAGS = $(OTHER_LDFLAGS)
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-Prova
PODS_ROOT = ${SRCROOT}/Pods